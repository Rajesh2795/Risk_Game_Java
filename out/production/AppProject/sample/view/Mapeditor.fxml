<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="anchorPane" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: Gainsboro;" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controller.MapEditorController">
   <children>
      <Label layoutX="322.0" layoutY="28.0" text="Choose Map" textFill="#0e618c">
         <font>
            <Font name="System Bold Italic" size="18.0" />
         </font></Label>
      <Button layoutX="449.0" layoutY="26.0" mnemonicParsing="false" prefWidth="50.0" style="-fx-background-color: LightSlategray;" text="Map" textFill="WHITE"
               onMouseClicked="#chooseMap"/>
      <TextArea fx:id="textArea" layoutX="23.0" layoutY="73.0" prefHeight="539.0" prefWidth="851.0">
         <font>
            <Font size="14.0" />
         </font>
      </TextArea>
      <Button fx:id="saveButton" layoutX="361.0" layoutY="647.0" mnemonicParsing="false" style="-fx-background-color: LightSlateGray;" text="Save" textFill="WHITE"
              onMouseClicked="#saveContent"/>
      <Button fx:id="closeButton" layoutX="467.0" layoutY="647.0" mnemonicParsing="false" style="-fx-background-color: LightSlateGray;" text="Cancel" textFill="WHITE"
              onMouseClicked="#cancelAction"/>
   </children>
</AnchorPane>
